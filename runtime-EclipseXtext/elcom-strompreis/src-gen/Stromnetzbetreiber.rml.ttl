PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
PREFIX ql: <http://semweb.mmlab.be/ns/ql#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>

<#StromnetzMapping>
	rml:logicalSource [
		rml:source "http://www.example.com/Schweizerische Gemeinden und zustï¿½ndige Stromnetzbetreiber (Stand 07.09.2018).csv" ;
		rml:referenceFormulation ql:CSV
	];
	
	rr:subjectMap [
		rr:template "http://elcom.zazuko.com/stormnetz/{NameUrl}" ;
		rr:class schema:Organization ;
	];
	
	rr:predicateObjectMap [
		rr:predicate rdfs:label ;
		rr:objectMap [
			rml:reference "Name" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:address ;
		rr:objectMap [
			rml:reference "Adresse" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:postalCode ;
		rr:objectMap [
			rml:reference "PLZ" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:addressLocality ;
		rr:objectMap [
			rr:template "http://classifications.data.admin.ch/municipality/{Ort}" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:areaServed ;
		rr:objectMap [
			rr:template "http://classifications.data.admin.ch/municipality/{Gde-Nr.}" ;
		];
	]
.