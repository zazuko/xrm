output carml

map FooMapping from KEYWORDSOURCE {
	subject template "http://foo.example.com/{0}" with id;

	types
		employee.Employee

	properties
		thing.color constant "blue";
		thing.size from ^map;
		// TODO get 'multi-reference' in code completion (due to type carml)
		// TODO get 'parent-map' in code completion (due to type rmlish)
		thing.weight multi-reference from ^map; 
}

logical-source KEYWORDSOURCE {
	type xml
	source "KWS"
	
	xml-namespace-extension SomeXmlNsExtension

	referenceables
		id
		^map
}

xml-namespace-extension SomeXmlNsExtension {
	prefix "exa" "http://www.example.com/audios/1.0/"
	// TODO check generated carmlfoo-mapping.carml.ttl --> carml:declaresNamespace should only be generated for 'employee', since this is the only prefix really used by the mapping
	prefix "employee" "http://www.example.com/audios/1.0/"
	prefix "exb" "http://www.example.com/books/1.0/"

	// TODO uncomment next line --> two errors for duplcated label on all prefixes with "exa"
	//	prefix "exa" "http://www.example.com/audios/1.0/"

	// TODO uncomment next line --> error because of ':' in label
	// prefix "exb:" "http://www.example.com/books/1.0/"
}

