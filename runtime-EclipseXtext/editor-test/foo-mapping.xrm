map FooMapping from KEYWORDSOURCE {
	subject template "http://foo.example.com/{0}" with id;

	types
		thing:Thing

	properties
		thing:color constant "blue"

		keyword:no from ^map
		keyword:^map from ^map
}

logical-source KEYWORDSOURCE {
	type xml
	source "KWS"

	referenceables
		id
		^map
}

vocabulary keyword {
	prefix "employee:" "http://example.com/employee"

	properties
		no
		^map
}

