output r2rml

map FooMapping from KEYWORDSOURCE {
	subject template "http://foo.example.com/{0}" with id;

	types
		employee:Employee

	properties
		thing:color constant "blue";
		keyword:no from ^map;
		keyword:^map from ^map;
}

map TestImpactMapping from SomeSourceGroup.foo {
	subject template "http://foo.example.com/{0}" with id;

	types
		employee:Employee

	properties
		thing:color constant "blue";
		keyword:^map from bar;
}

logical-source KEYWORDSOURCE {
	type xml
	source "KWS"

	referenceables
		id
		^map
}

vocabulary keyword {
	prefix "employee:" "http://example.com/employee"

	properties
		no
		^map
}

