source-group SomeSourceGroup {
	// TODO move line 'dialect' into logicalSource below 'source' --> expect no changes in generated files' (on a project.clean())
	dialect SomeDialect
	logical-source foo {
		type csv
		source "http://www.example.com/Foo.csv"

		referenceables
			id
			foo
			bar
	}

}

dialect SomeDialect {
	delimiter ","
	commentPrefix "#"
	doubleQuote true
	encoding "utf-8"
	header true
	headerRowCount 1
	lineTerminators "\\r\\n"
	quoteChar '\\"'
	skipBlankRows false
	skipColumns 0
	skipInitialSpace false
	skipRows 0
	trim false
}
