PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
PREFIX ql: <http://semweb.mmlab.be/ns/ql#>
PREFIX employee: <http://example.com/employee>
PREFIX employee: <http://example.com/employee>
PREFIX thing: <http://foobar.com/things/thing/>

<#FooMapping> a rr:TriplesMap ;
	rml:logicalSource [
		rml:source "KWS" ;
		rml:referenceFormulation ql:XPath
	];
	
	rr:subjectMap [
		rr:template "foo" ;
		rr:class employee:Employee ;
	];
	
	rr:predicateObjectMap [
		rr:predicate thing:color ;
		rr:objectMap [
			rr:constant "blue" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate employee:no ;
		rr:objectMap [
			rml:reference "map" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate employee:map ;
		rr:objectMap [
			rml:reference "map" ;
		];
	]
.
<#ReferenceTemplateFromOtherFile> a rr:TriplesMap ;
	rml:logicalSource [
		rml:query """EMP""" ;
		rml:referenceFormulation ql:XPath ;
		rml:iterator "/Employees/Employee" ;
	];
	
	rr:subjectMap [
		rr:template "http://airport.example.com/{EMPNO}" ;
	]
	
.
<#TestImpactMapping> a rr:TriplesMap ;
	rml:logicalSource [
		rml:source "http://www.example.com/Foo.xml" ;
		rml:referenceFormulation ql:CSV
	];
	
	rr:subjectMap [
		rr:template "http://foo.example.com/{id}" ;
		rr:class employee:Employee ;
	];
	
	rr:predicateObjectMap [
		rr:predicate thing:color ;
		rr:objectMap [
			rr:constant "blue" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate employee:map ;
		rr:objectMap [
			rml:reference "bar" ;
		];
	]
.