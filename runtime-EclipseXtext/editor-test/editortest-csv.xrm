}

map EmployeeMapping2 from EMPLOYEE {
	subject template "http://airport.example.com/{0}" with id;

	types
		employee:Employee

	properties
		employee:no from EMPNO;
		// TODO codeCompletion between EMPNO and ; must not give 'as'
		keyword:^map template "{0}" with EMPNO;
		// TODO uncomment next line, warning 'Literal' + quickfix
		// keyword:^map template "{0}" with EMPNO as Literal;
		
}


logical-source EMPLOYEE {
	type csv
	source "EMP"
	iterator "/Employees/Employee"

	referenceables
		id
		EMPNO
// TODO uncomment next line. error: null "X" is only supported on type cfs
