output csvw

// TODO compare generated files:
//// editortest-csv-onemappingonly.csv.meta.json
//// editortest-csv.csv.meta.json
//// --> onemapping does not show field "tables"
map EmployeeMapping from EMPLOYEE {
	subject template "http://airport.example.com/{0}" with id;

	types
		employee.Employee

	properties
		employee.no from EMPNO;
}

logical-source EMPLOYEE {
	type csv
	// TODO code completion sourceType==csv: yield 'dialect', but not 'xml-namespace-extension'
	source "EMP"
	iterator "/Employees/Employee"

	referenceables
		id
		EMPNO
}

vocabulary employee {
	prefix "employee" "http://example.com/employee"

	classes
		Employee

	properties
		no
}

