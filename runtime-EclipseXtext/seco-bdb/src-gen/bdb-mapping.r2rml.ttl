<#Amt>
	rr:logicalTable [ rr:tableName "t_amt" ];
	
	rr:subjectMap [
		rr:template "https://permits.zazukoians.org/agencies/{AMT_ID}" ;
		rr:class schema:GovernmentOrganization ;
	];
	
	rr:predicateObjectMap [
		rr:predicate schema:alternateName ;
