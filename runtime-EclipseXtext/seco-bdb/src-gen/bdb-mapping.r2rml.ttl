			rr:column "AMT_ABK_DE" ;
			rr:language "de" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:alternateName ;
		rr:objectMap [
			rr:column "AMT_ABK_FR" ;
			rr:language "fr" ;
