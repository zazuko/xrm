<#Amt>
	rml:logicalSource [
		rml:source "t_amt" ;
		rml:referenceFormulation rr:SQL2008
	];
	
	rr:subjectMap [
		rr:template "https://permits.zazukoians.org/agencies/{AMT_ID}";
		rr:class schema:GovernmentOrganization ;
	];
	
	rr:predicateObjectMap [
		rr:predicate schema:alternateName ;
		rr:objectMap [
			rml:reference "AMT_ABK_DE" ;
			rr:language "de" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:alternateName ;
		rr:objectMap [
			rml:reference "AMT_ABK_FR" ;
			rr:language "fr" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:alternateName ;
		rr:objectMap [
			rml:reference "AMT_ABK_EN" ;
			rr:language "en" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:alternateName ;
		rr:objectMap [
			rml:reference "AMT_ABK_IT" ;
			rr:language "it" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:name ;
		rr:objectMap [
			rml:reference "AMT_NAME_DE" ;
			rr:language "de" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:name ;
		rr:objectMap [
			rml:reference "AMT_NAME_FR" ;
			rr:language "fr" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:name ;
		rr:objectMap [
			rml:reference "AMT_NAME_EN" ;
			rr:language "en" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:name ;
		rr:objectMap [
			rml:reference "AMT_NAME_IT" ;
			rr:language "it" ;
		];
	]
.
