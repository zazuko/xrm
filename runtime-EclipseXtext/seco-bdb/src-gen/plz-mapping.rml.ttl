PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
PREFIX ql: <http://semweb.mmlab.be/ns/ql#>
PREFIX ex: <http://schema.example.org/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<#Ort>
	rml:logicalSource [
		rml:source "plz_verzeichnis_v2.csv" ;
		rml:referenceFormulation ql:CSV
	];
	
	rr:subjectMap [
		rr:template "http://example.org/ort/{ONRP}" ;
		rr:class ex:Ort ;
	];
	
	rr:predicateObjectMap [
		rr:predicate ex:onrp ;
		rr:objectMap [
			rml:reference "ONRP" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:bfsnr ;
		rr:objectMap [
			rml:reference "BFSNR" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:plz ;
		rr:objectMap [
			rml:reference "POSTLEITZAHL" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:zz ;
		rr:objectMap [
			rml:reference "PLZ_ZZ" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:bezeichnung ;
		rr:objectMap [
			rml:reference "ORTBEZ18" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:bezeichnung ;
		rr:objectMap [
			rml:reference "ORTBEZ27" ;
		];
	]
.