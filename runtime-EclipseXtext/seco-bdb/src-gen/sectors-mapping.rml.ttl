PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
PREFIX ql: <http://semweb.mmlab.be/ns/ql#>
PREFIX bdb: <https://permits.ld.admin.ch/schema/>
PREFIX ex: <http://schema.example.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<#Sector>
	rml:logicalSource [
		rml:source "easygov-sectors.csv" ;
		rml:referenceFormulation ql:CSV
	];
	
	rr:subjectMap [
		rr:template "https://permits.zazukoians.org/sectors/{CODE}";
		rr:class bdb:Sector ;
		rr:class skos:Concept ;
	];
	
	rr:predicateObjectMap [
		rr:predicate ex:internalKey ;
		rr:objectMap [
			rml:reference "PK fiz" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:position ;
		rr:objectMap [
			rml:reference "POS" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:code ;
		rr:objectMap [
			rml:reference "CODE" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate skos:notation ;
		rr:objectMap [
			rr:template "{CODE}" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:fkParent ;
		rr:objectMap [
			rml:reference "LN_PARENT" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate skos:prefLabel ;
		rr:objectMap [
			rml:reference "DE" ;
			rr:language "de" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate skos:prefLabel ;
		rr:objectMap [
			rml:reference "EN" ;
			rr:language "en" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate skos:prefLabel ;
		rr:objectMap [
			rml:reference "FR" ;
			rr:language "fr" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate skos:prefLabel ;
		rr:objectMap [
			rml:reference "IT" ;
			rr:language "it" ;
		];
	]
.