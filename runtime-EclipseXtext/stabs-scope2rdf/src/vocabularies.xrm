language-tags {
	de
}

datatypes xsd {
	prefix "xsd:" "http://www.w3.org/2001/XMLSchema#"
	int
	double
}

vocabulary dc {
	prefix "dc:" "http://purl.org/dc/terms/"
	
	classes
	    BibliographicResource 
	
	properties
	    creator
	    title
		relation
}

/** Friend of a friend
    attemt to model partners and (ablieferungs-)projects
*/
vocabulary foaf {
	prefix "foaf:" "http://xmlns.com/foaf/0.1/"
	
	classes
	    Agent
	    Group
	    Person
	    Organization
	    Project
	
	properties
		made
		name
		openid
}

// Web annotation ontology
vocabulary oa {
	prefix "oa:"  "http://www.w3.org/ns/oa#"
	
	classes
		Annotation
		
	properties
		hasBody
		hasTarget
}

vocabulary ric {
    prefix "ric:" "http://purl.org/ica/ric#"
    classes
        Record
    	RecordSet
    	
    properties
    	something
    	title
    	scopeAndContent
    	contentType
    	qualityOfInformation
    	date
    	characteristic_Physical
		// TODO '-' in IDs -> characteristic-Physical    
}

vocabulary skos {
	prefix "skos:" "http://www.w3.org/2004/02/skos/core#"
	classes
		Concept
		
	properties
		notation
		prefLabel
		inScheme
}

vocabulary schema {
	
	prefix "schema:" "http://schema.org/"
	
	classes 
		Person
	
	properties
	 place
	
}

